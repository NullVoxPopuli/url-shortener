    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.0.0",
    "@embroider/vite": "^1.0.0",
    "@glint/ember-tsc": "^1.0.0",
    "@glint/template": "1.7.3",
    "@glint/tsserver-plugin": "^2.0.0",
    "@nullvoxpopuli/eslint-configs": "^5.0.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "@warp-drive/build-config": "5.8.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.1",
    "ember-cli": "~6.9.1",
    "ember-qunit": "^9.0.4",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.2",
    "prettier": "github:jurgenwerk/prettier#glimmer-embed-style-wip",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "ember": {
    "edition": "octane"
  },
