{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "rebuild": "./rebuild.sh",
    "start": "node bin/server.js",
    "build": "node ace build --ignore-ts-errors",
    "ace": "NODE_OPTIONS='--no-deprecation' node ace ",
    "dev": "NODE_OPTIONS='--no-deprecation' node ace serve --hmr",
    "dev:offline": "NODE_ENV=development DB_CONNECTION=sqlite3 NODE_OPTIONS='--no-deprecation' node ace serve --hmr",
    "db:offline:reset": "DB_CONNECTION=sqlite3 node ace migration:fresh",
    "test": "NODE_OPTIONS='--no-deprecation' node ace test",
    "test:watch": "NODE_OPTIONS='--no-deprecation' node ace test --watch",
    "test:debug": "./test-debug.sh",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#consts": "./app/consts.js",
    "#jsonapi": "./app/jsonapi/index.js",
    "#jsonapi/*": "./app/jsonapi/*.js",
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#utils/*": "./utils/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@faker-js/faker": "^8.4.1",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/browser-client": "^2.0.3",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.7.23",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "@types/uuid": "^10.0.0",
    "better-sqlite3": "^11.2.1",
    "chai": "^5.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "hot-hook": "^0.2.6",
    "playwright": "^1.46.1",
    "prettier": "^3.3.3",
    "prettier-edgejs": "^0.2.27",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0"
  },
  "dependencies": {
    "@adonisjs/ally": "^5.0.2",
    "@adonisjs/auth": "^9.2.1",
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.2.0",
    "@adonisjs/session": "^7.4.1",
    "@adonisjs/static": "^1.1.1",
    "@nullvoxpopuli/url-compression": "workspace:^",
    "@vinejs/vine": "^2.0.0",
    "edge.js": "^6.0.2",
    "find-up": "^7.0.0",
    "luxon": "^3.5.0",
    "node-confirm": "^1.0.2",
    "pg": "^8.12.0",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2",
    "ts-japi": "^1.11.4"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "packageManager": "pnpm@9.10.0",
  "volta": {
    "extends": "../package.json"
  }
}
